{
    "name": "NSFWBlur",
    "version": "2.0.0",
    "author": "OnikaGPT 4o, Christian Daniel Jensen, ChatGPT-5.2",
    "url": "https://www.mediawiki.org/wiki/Extension:NSFW_Filter",
    "descriptionmsg": "nsfwblur-desc",
    "type": "Hooks",
    "manifest_version": 2,
    "requires": {
        "MediaWiki": ">= 1.43.0"
    },
    "AutoloadClasses": {
        "NSFWBlur\\Hooks": "includes/Hooks.php"
    },
    "ResourceModules": {
        "ext.nsfwblur.top": {
            "localBasePath": ".",
            "remoteExtPath": "NsfwFilter",
            "position": "top",
            "dependencies": [
            "mediawiki.util",
            "mediawiki.Uri",
            "mediawiki.Title"
            ],
            "scripts": [
            "modules/nsfw-blur.top.js"
            ],
            "targets": [
            "desktop",
            "mobile"
            ]
        },
        "ext.nsfwblur": {
            "localBasePath": ".",
            "remoteExtPath": "NsfwFilter",
            "dependencies": [
            "user.options"
            ],
            "styles": [
            "modules/nsfw-blur.css"
            ],
            "scripts": [
            "modules/nsfw-blur.js"
            ],
            "targets": [
            "desktop",
            "mobile"
            ]
        }
    },
    "MessagesDirs": {
        "NSFWBlur": [
            "i18n"
        ]
    },
    "DefaultUserOptions": {
        "nsfwblurred": false
    },
    "Hooks": {
        "BeforePageDisplay": [
            "NSFWBlur\\Hooks::onBeforePageDisplay"
        ],
        "OutputPageParserOutput": [
            "NSFWBlur\\Hooks::onOutputPageParserOutput"
        ],
        "ImageBeforeProduceHTML": [
            "NSFWBlur\\Hooks::onImageBeforeProduceHTML"
        ],
        "GetPreferences": [
            "NSFWBlur\\Hooks::onGetPreferences"
        ],
        "PageRenderingHash": [
            "NSFWBlur\\Hooks::onPageRenderingHash"
        ],
        "UserGetDefaultOptions": [
            "NSFWBlur\\Hooks::onUserGetDefaultOptions"
        ],
        "UserSaveOptions": [
            "NSFWBlur\\Hooks::onUserSaveOptions"
        ]
    }
}
